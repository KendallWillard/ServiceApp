<h1>Appointments Index Page</h1>

<ul>
  <% @appointments.each do |appointment| %>
  <li>
    Date: <%= appointment.date %> <br>
    Time: <%= appointment.time %> <br>
    Description: <%= appointment.description %> <br>
    Homeowner: <%= @homeowner.first_name %> <%= @homeowner.last_name %>
    
  </li>
  <% end %>
</ul>
